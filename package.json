{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.19",
    "antd": "3.8.4",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "qs": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^4.0.1",
    "swiper": "^6.2.0",
    "umi": "^3.2.19",
    "umi-request": "latest",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0"
  }
}
